<!DOCTYPE html>
<html lang="zh-cn">
    <head>
        <meta charset="utf-8" />
        <meta name="renderer" content="webkit" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge"></meta> 
        <link rel="dns-prefetch" href="//ued.shoy.com"/>
        <title>@ViewBag.Title - 健康管理中心 后台管理系统</title>
        <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1" />
        <!--苹果Icon-->
        <link href="" rel="apple-touch-icon-precomposed" />
        <link type="text/css" rel="stylesheet" href="//ued.shoy.com/css/reset.min.css" media="all" />
        <link href="//ued.shoy.com/css/tjzx/m/m-base.min.css" rel="stylesheet" type="text/css" media="all" />
        <!--[if lt IE 9]>
            <script src="//ued.shoy.com/js/html5shiv.min.js"></script>
        <![endif]-->
        <script type="text/javascript" src="//ued.shoy.com/plugs/artDialog/v6/lib/sea.js" charset="utf-8"></script>
        <script type="text/javascript">
            seajs.config({
                alias: {
                    "jquery": "jquery-1.10.2.js",
                    "dialog": "http://ued.shoy.com/plugs/artDialog/v6/src/dialog",
                    "plus": "http://ued.shoy.com/plugs/artDialog/v6/src/dialog-plus"
                },
                preload: ["plus"]
            });
            (function () {
                /**
                * 暴露到全局
                */
                seajs.use("plus", function (dialog) {
                    window.artDialog = window.D = dialog;
                });
            })();
        </script>
        @RenderSection("Header", false)
    </head>
    <body>
        <script type="text/javascript" src="//ued.shoy.com/js/singer.js"></script>
        @RenderBody()
        <script type="text/javascript" src="//ued.shoy.com/js/jquery.min.js"></script>
        <script type="text/javascript" src="//ued.shoy.com/js/hTemplate.min.js" charset="utf-8"></script>
        @RenderSection("Footer",false)
    </body>
</html>
